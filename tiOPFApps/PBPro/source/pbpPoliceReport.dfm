  object ContractQuery: TIBQuery
    Database = Database
    Transaction = IBTransaction1
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select * from contract'
      
        'left outer join client_address on contract.client_address_oid = ' +
        'client_address.oid'
      'left outer join client on contract.client_oid = client.oid'
      'where'
      
        '((contract.start_date >= :StartDate) and (contract.start_date <=' +
        ' :EndDate)) or'
      
        '((Contract.end_date >= :StartDate) and (Contract.end_date <= :En' +
        'dDate))'
      '')
    Left = 152
    Top = 104
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'StartDate'
        ParamType = ptUnknown
