Source: "%PATH_BIN%\*.xsl"; DestDir: "{app}"; Flags: ignoreversion
Source: "%PATH_BIN%\*.dll"; DestDir: "{app}"; Flags: ignoreversion

[Icons]
Name: "{group}\TechInsite Group Mail"; Filename: "{app}\%EXE_NAME%.exe"
Name: "{group}\{cm:UninstallProgram,TechInsite Group Mail}"; Filename: "{uninstallexe}"
Name: "{userdesktop}\TechInsite Group Mail"; Filename: "{app}\%EXE_NAME%.exe"; Tasks: desktopicon

[Run]
Filename: "{app}\%EXE_NAME%.exe"; Description: "{cm:LaunchProgram,TechInsite Group Mail}"; Flags: nowait postinstall skipifsilent

