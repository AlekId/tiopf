[Setup]
AppName=TechInsite Group Mail
AppVerName=TechInsite Group Mail %RELEASE_VERSION%
AppPublisher=TechInsite Pty Ltd
AppPublisherURL=www.techinsite.com.au
AppSupportURL=www.techinsite.com.au
AppUpdatesURL=www.techinsite.com.au
AppMutex=tiGroupMail
DefaultDirName={pf}\tiGroupMail
UsePreviousAppDir=yes
DefaultGroupName=TechInsite Group Mail
LicenseFile=.\Licence.txt
Compression=lzma
SolidCompression=yes
OutputDir=%PATH_SETUP%\Output
OutputBaseFileName=tiGroupMail_Setup_%RELEASE_VERSION_FILENAME%


[Tasks]
