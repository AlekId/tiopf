    object Memo1: TMemo
      Left = 4
      Top = 92
      Width = 221
      Height = 296
      Anchors = [akLeft, akTop, akRight, akBottom]
      Lines.Strings = (
        'Browsing a TLlist of TPersistent'
        ''
        'At runtime:'
        '* Click on a column heading to sort the '
        'list.'
        ''
        '* Right click to insert, edit or delete.'
        ''
        '* Column widths will be automatically set.'
        ''
        '* There will be a red X beside weekend '
        'days.'
        ''
        '* The font will be red if the integer value'
        '  Is odd, and blue if even.'
        ''
        'At design time:'
        ''
        '* Edit the ListColumns property to change '
        'the column heading or display mask.'
        ''
        '* Edit the OnGetFont or OnGetImageIndex '
        'events to change the row display '
        'properties.'
        ''
        '* Place code in the OnFilterData event to '
        'apply a filter.'
        ''
        '* Place code in the OnDelete, OnInsert or '
        'OnEdit events to manage deletes, inserts '
        'and edits.')
      ParentColor = True
      ReadOnly = True
      ScrollBars = ssVertical
      TabOrder = 2
    end
