<?xml version="1.0"?>
<CONFIG>
  <ProjectOptions>
    <PathDelim Value="/"/>
    <Version Value="6"/>
    <General>
      <Flags>
        <SaveOnlyProjectUnits Value="True"/>
      </Flags>
      <SessionStorage Value="InProjectDir"/>
      <MainUnit Value="0"/>
      <TargetFileExt Value=""/>
    </General>
    <VersionInfo>
      <ProjectVersion Value=""/>
    </VersionInfo>
    <PublishOptions>
      <Version Value="2"/>
      <IncludeFileFilter Value="*.(pas|pp|inc|lfm|lpr|lrs|lpi|lpk|sh|xml)"/>
      <ExcludeFileFilter Value="*.(bak|ppu|ppw|o|so);*~;backup"/>
    </PublishOptions>
    <RunParams>
      <local>
        <FormatVersion Value="1"/>
        <LaunchingApplication PathPlusParams="/usr/X11R6/bin/xterm -T 'Lazarus Run Output' -e $(LazarusDir)/tools/runwait.sh $(TargetCmdLine)"/>
      </local>
    </RunParams>
    <RequiredPackages Count="3">
      <Item1>
        <PackageName Value="FCL"/>
      </Item1>
      <Item2>
        <PackageName Value="FPCUnitConsoleRunner"/>
      </Item2>
      <Item3>
        <PackageName Value="tiOPF"/>
      </Item3>
    </RequiredPackages>
    <Units Count="2">
      <Unit0>
        <Filename Value="fpcunittiopftext.lpr"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="fpcunittiopftext"/>
      </Unit0>
      <Unit1>
        <Filename Value="../Common/tiTestDependencies.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="tiTestDependencies"/>
      </Unit1>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="8"/>
    <Target>
      <Filename Value="fpcUnitTIOPFText.exe"/>
    </Target>
    <SearchPaths>
      <IncludeFiles Value="../../Core/"/>
      <OtherUnitFiles Value="../Common/;../Tests/"/>
    </SearchPaths>
    <Other>
      <CustomOptions Value="-FU_Dcu
-dUseCThreads
-dLINK_TAB
-dLINK_CSV
-dLINK_FBL
-dLINK_XMLLIGHTx
-dLINK_SQLDB_IBx
"/>
      <CompilerPath Value="$(CompPath)"/>
    </Other>
  </CompilerOptions>
</CONFIG>
